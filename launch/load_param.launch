<launch>
  <!-- Hardware config -->
  <rosparam file="$(find launcher)/param/hardware.yaml" ns="hardware"/>
  <!-- EBand config -->
  <rosparam file="$(find launcher)/param/eband_local_planner.yaml" ns="move_base" />
  <!-- Global costmap -->
  <rosparam file="$(find launcher)/param/global_costmap.yaml" ns="move_base"/>
  <!-- Local costmap -->
  <rosparam file="$(find launcher)/param/local_costmap.yaml" ns="move_base"/>
  <!-- AMCL init pose -->
  <rosparam file="$(find launcher)/param/amcl_init.yaml" ns="amcl"/>
</launch>
